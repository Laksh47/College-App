<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mynotify"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mynotify.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mynotify.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.example.mynotify.DeptActivity"
            android:label="@string/title_activity_dept" >
        </activity>
        <activity
            android:name="com.example.mynotify.SigActivity"
            android:label="@string/title_activity_sig" >
        </activity>
        <activity
            android:name="com.example.mynotify.PlacedActivity"
            android:label="@string/title_activity_placed" >
        </activity>
        <activity
            android:name="com.example.mynotify.RepresentActivity"
            android:label="@string/title_activity_represent" >
        </activity>
        <activity
            android:name="com.example.mynotify.RepActivity"
            android:label="@string/title_activity_rep" >
        </activity>
        <activity
            android:name="com.example.mynotify.InsertActivity"
            android:label="@string/title_activity_insert" >
        </activity>
        <activity
            android:name="com.example.mynotify.DisplayActivity"
            android:label="@string/title_activity_display" >
        </activity>
        <activity
            android:name="com.example.mynotify.UpdateActivity"
            android:label="@string/title_activity_update" >
        </activity>
        <activity
            android:name="com.example.mynotify.DeleteActivity"
            android:label="@string/title_activity_delete" >
        </activity>
    </application>

</manifest>
